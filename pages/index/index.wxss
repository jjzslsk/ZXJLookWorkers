.class-name{text-align: center;font-size: 0.8em;margin-top: 0.5px;}
.item-child{background: #FFFFFF;border-radius: 5px; border-bottom: -5px;}

.activity{padding: 10px;box-sizing:border-box;margin-top: -5px;}
