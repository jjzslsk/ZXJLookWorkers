/* pages/myOrders/myOrders.wxss */
.body{width: 100%;height: 100vh;}
.list-info{font-size: 0.8em;padding: 10px 15px;border-bottom: 10px #F5F5F5 solid;}
.text-right{text-align: right;}
.title{font-size: 1em;}
.item-title{color: #999999;}
.title-item{background: #FFFFFF;padding: 10px 0;}
.child-item{background: #F5F5F5;padding: 10px 15px;}
.scroll-area {height: calc(100% - 4.6rem);}
.plage-img{margin: 0 auto;}
.bg-body{background: #F5F5F5;width:100%;}
.order-no{color: #259B24}
.status{text-align: center;background: #F1F1F1;border-radius: 10px;font-size: 0.6em;padding:5px;}
.padded-10{padding: 5px 15px;box-sizing:border-box;}
.status-item{box-sizing:border-box;overflow: hidden;border-bottom: 1px #DDDDDD solid;height: 4.6rem}
.status-act{background: #259B24;color: #FFFFFF;}

.order-info{background: #EEEEEE;width:100%;padding: 5px 10px;margin-top: 5px;}
.color-tips{color:#999999;}
.text-right{text-align: right;}
.no-data{padding: 15px;text-align: center;font-size: 0.8em;color: #999999;}

.status-count{position: absolute;width: 20px;height: 20px;line-height: 20px;text-align: center;background: red;color: #FFFFFF;font-size: 0.6em;border-radius: 50%;right:0;margin-top: -5px;}
.class-item{margin-right: 10px;box-sizing:border-box;}
.order-count{color: #666666;}
.padded-5{padding: 5px;box-sizing:border-box;}
.invite-order{background: #259B24;font-size: 0.8em;color: #FFFFFF;padding: 1px 5px;border-radius: 2px;margin-left: 5px;}

.aui-flex-col,
.aui-flex-row {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-wrap: wrap;
            flex-wrap: wrap;
}
.aui-flex-col:before,
.aui-flex-row:before,
.aui-flex-col:after,
.aui-flex-row:after {
    content: " ";
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    box-sizing: border-box;
}
.aui-flex-col:after,
.aui-flex-row:after {
  clear: both;
}
.aui-flex-col {
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
            flex-direction: row;
    padding: 5px 0;
}
.aui-flex-row {
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
            flex-direction: column;
}
